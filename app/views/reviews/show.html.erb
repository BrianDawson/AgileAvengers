<p id="notice"><%= notice %></p>

<strong>User Overall Rating:</strong> <%= rating_for @review.property, "overall_rating", disabled: true %>

<p>
  <strong>Review:</strong>
  <%= @review.review %>
</p>

<div style="float:left;">
  <%= button_to 'Like', like_property_review_path(@review.property, @review), method: :put %>
</div>
<div style="float:left;">
  <%= button_to 'Dislike', dislike_property_review_path(@review.property, @review), method: :put %>
</div>  
<%= @review.score %>

<br><br>

<%= link_to 'Edit', edit_property_review_path(@review.property, @review) %> |
<%= link_to 'Back', property_reviews_path(@review.property) %>